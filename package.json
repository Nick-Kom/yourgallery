{
  "name": "your-gallery",
  "version": "1.0.0",
  "description": "Angular spa your-gallery",
  "scripts": {
    "start": "cross-env NODE_ENV=development  webpack-dev-server --inline --progress --config config/webpack.dev.js ",
    "build": "rimraf build && webpack --config config/webpack.prod.js --progress --profile --bail",
    "prod": "npm run prod:build && npm run prod:launch",
    "prod:build": "rimraf build && cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "prod:launch": "node ./build/www.js"
  },
